{
  "name": "conjurate",
  "version": "0.3.2",
  "description": "Easy scaffolding tool",
  "main": "cli.js",
  "bin": {
    "conjurate": "./cli.js"
  },
  "scripts": {
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "xo",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "pretty": "pretty-quick"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/filipelinhares/conjurate.git"
  },
  "author": "Filipe Linhares",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/filipelinhares/conjurate/issues"
  },
  "homepage": "https://github.com/filipelinhares/conjurate#readme",
  "dependencies": {
    "chalk": "^3.0.0",
    "change-case": "^3.1.0",
    "fs-extra": "^8.1.0",
    "mri": "^1.1.4",
    "prompts": "^2.3.0",
    "read-pkg-up": "^7.0.0",
    "resolve-pkg": "^2.0.0",
    "signale": "^1.4.0",
    "vinyl-fs": "^3.0.3",
    "write-pkg": "^4.0.0"
  },
  "devDependencies": {
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "fileshelf": "^0.1.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "xo": "^0.25.3"
  },
  "xo": {
    "space": true,
    "envs": [
      "jest"
    ],
    "extends": [
      "prettier"
    ],
    "plugins": [
      "prettier"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && pretty-quick --staged"
    }
  }
}
