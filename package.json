{
  "name": "conjurate",
  "version": "0.4.0",
  "description": "Easy scaffolding tool",
  "main": "cli.js",
  "bin": {
    "conjurate": "./cli.js"
  },
  "scripts": {
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "standard --env jest",
    "test:unit": "jest",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/filipelinhares/conjurate.git"
  },
  "author": "Filipe Linhares",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/filipelinhares/conjurate/issues"
  },
  "homepage": "https://github.com/filipelinhares/conjurate#readme",
  "dependencies": {
    "chalk": "^3.0.0",
    "change-case": "^3.1.0",
    "fs-extra": "^8.1.0",
    "mri": "^1.1.4",
    "prompts": "^2.3.0",
    "read-pkg-up": "^7.0.0",
    "resolve-pkg": "^2.0.0",
    "signale": "^1.4.0",
    "vinyl-fs": "^3.0.3",
    "write-pkg": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^6.7.2",
    "fileshelf": "^0.1.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "standard": "^14.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && standard --env jest --fix"
    }
  }
}
